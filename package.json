{
   "name": "puppeteer-browser-ready",
   "version": "0.1.3",
   "description": "Simple utility to go to a URL and wait for the HTTP response",
   "license": "MIT",
   "type": "module",
   "main": "dist/puppeteer-browser-ready.umd.cjs",
   "module": "dist/puppeteer-browser-ready.js",
   "types": "dist/puppeteer-browser-ready.d.ts",
   "files": [
      "dist"
   ],
   "repository": "github:center-key/puppeteer-browser-ready",
   "homepage": "https://github.com/center-key/puppeteer-browser-ready",
   "bugs": "https://github.com/center-key/puppeteer-browser-ready/issues",
   "docs": "https://github.com/center-key/puppeteer-browser-ready#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "browser",
      "http",
      "javascript",
      "mocha",
      "puppeteer",
      "ready",
      "response",
      "url"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "latedef": true,
      "nonbsp": true,
      "singleGroups": true,
      "undef": true,
      "unused": true,
      "mocha": true,
      "node": true
   },
   "eslintConfig": {
      "ignorePatterns": [
         "build",
         "dist",
         "node_modules"
      ],
      "root": true,
      "parser": "@typescript-eslint/parser",
      "plugins": [
         "@typescript-eslint"
      ],
      "extends": [
         "eslint:recommended",
         "plugin:@typescript-eslint/recommended"
      ]
   },
   "scripts": {
      "clean": "rimraf build dist **/.DS_Store && shx mkdir dist",
      "postclean": "tsc --version && npm run build-esm && npm run build-dts && npm run build-umd",
      "build-esm": "tsc --project . && shx cp build/*.js dist/",
      "build-dts": "name=$(basename $PWD) && shx head -n 1 build/*.js | cat - build/*.d.ts > dist/$name.d.ts",
      "build-umd": "name=$(basename $PWD) && tsc --project . --module UMD --outDir build/umd && shx cp build/umd/*.js dist/$name.umd.cjs",
      "pretest": "jshint spec/*.js && eslint --max-warnings 0 . --ext .ts && npm run clean",
      "test": "mocha spec/*-spec.js --timeout 5000"
   },
   "dependencies": {
      "cheerio": "^1.0.0-rc.5"
   },
   "peerDependencies": {
      "puppeteer": "^5"
   },
   "devDependencies": {
      "@types/cheerio": "~0.22",
      "@types/puppeteer": "~5.4",
      "@typescript-eslint/eslint-plugin": "~4.13",
      "@typescript-eslint/parser": "~4.13",
      "eslint": "~7.18",
      "jshint": "~2.12",
      "mocha": "~8.2",
      "puppeteer": "~5.5",
      "rimraf": "~3.0",
      "shx": "~0.3",
      "typescript": "~4.1"
   }
}
