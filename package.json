{
   "name": "puppeteer-browser-ready",
   "version": "0.3.3",
   "description": "Simple utility to go to a URL and wait for the HTTP response (written in TypeScript)",
   "license": "MIT",
   "type": "module",
   "module": "dist/puppeteer-browser-ready.js",
   "main": "dist/puppeteer-browser-ready.umd.cjs",
   "types": "dist/puppeteer-browser-ready.d.ts",
   "files": [
      "dist"
   ],
   "exports": {
      ".": {
         "import": "./dist/puppeteer-browser-ready.js",
         "require": "./dist/puppeteer-browser-ready.umd.cjs"
      },
      "./": "./dist/"
   },
   "repository": "github:center-key/puppeteer-browser-ready",
   "homepage": "https://github.com/center-key/puppeteer-browser-ready",
   "bugs": "https://github.com/center-key/puppeteer-browser-ready/issues",
   "docs": "https://github.com/center-key/puppeteer-browser-ready#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "browser",
      "http",
      "javascript",
      "mocha",
      "puppeteer",
      "puppeteer-browser-ready",
      "ready",
      "response",
      "url"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "latedef": true,
      "nonbsp": true,
      "singleGroups": true,
      "undef": true,
      "unused": true,
      "mocha": true,
      "node": true
   },
   "eslintConfig": {
      "ignorePatterns": [
         "build",
         "dist",
         "node_modules"
      ],
      "root": true,
      "parser": "@typescript-eslint/parser",
      "plugins": [
         "@typescript-eslint"
      ],
      "extends": [
         "eslint:recommended",
         "plugin:@typescript-eslint/recommended"
      ],
      "rules": {
         "@typescript-eslint/no-non-null-assertion": "off"
      }
   },
   "scripts": {
      "clean": "    rimraf build dist **/.DS_Store && shx mkdir dist",
      "postclean": "npm run build-esm && npm run build-dts && npm run build-umd",
      "build-esm": "tsc && shx cp build/*.js dist/",
      "build-dts": "name=$(basename $PWD) && shx head -n 1 build/*.js | cat - build/*.d.ts > dist/$name.d.ts",
      "build-umd": "name=$(basename $PWD) && tsc --module UMD --outDir build/umd && shx cp build/umd/*.js dist/$name.umd.cjs",
      "pretest": "  jshint . --exclude '**/node_modules,build,dist' && eslint --max-warnings 0 . --ext .ts && w3c-html-validator && npm run clean",
      "test": "     mocha spec/*.spec.js --timeout 5000"
   },
   "peerDependencies": {
      "puppeteer": "^5 || ^6 || ^7 || ^8 || ^9 || ^10"
   },
   "dependencies": {
      "cheerio": "~1.0.0-rc.10",
      "express": "~4.17",
      "http-terminator": "~3.0"
   },
   "devDependencies": {
      "@types/cheerio": "~0.22",
      "@types/express": "~4.17",
      "@types/node": "~16.9",
      "@typescript-eslint/eslint-plugin": "~4.31",
      "@typescript-eslint/parser": "~4.31",
      "assert-deep-strict-equal": "~0.0",
      "eslint": "~7.32",
      "jshint": "~2.13",
      "mocha": "~9.1",
      "open": "~8.2",
      "puppeteer": "~10.4",
      "rimraf": "~3.0",
      "shx": "~0.3",
      "typescript": "~4.4",
      "w3c-html-validator": "~0.7"
   }
}
