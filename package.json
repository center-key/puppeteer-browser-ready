{
   "name": "puppeteer-browser-ready",
   "version": "0.5.0",
   "description": "Simple utility to go to a URL and wait for the HTTP response (written in TypeScript)",
   "license": "MIT",
   "type": "module",
   "module": "dist/puppeteer-browser-ready.js",
   "main": "dist/puppeteer-browser-ready.umd.cjs",
   "types": "dist/puppeteer-browser-ready.d.ts",
   "files": [
      "dist"
   ],
   "exports": {
      ".": {
         "import": "./dist/puppeteer-browser-ready.js",
         "require": "./dist/puppeteer-browser-ready.umd.cjs"
      },
      "./": "./dist/"
   },
   "repository": "github:center-key/puppeteer-browser-ready",
   "homepage": "https://github.com/center-key/puppeteer-browser-ready",
   "bugs": "https://github.com/center-key/puppeteer-browser-ready/issues",
   "docs": "https://github.com/center-key/puppeteer-browser-ready#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "browser",
      "http",
      "javascript",
      "mocha",
      "puppeteer",
      "puppeteer-browser-ready",
      "ready",
      "response",
      "url"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "latedef": true,
      "nonbsp": true,
      "singleGroups": true,
      "undef": true,
      "unused": true,
      "mocha": true,
      "node": true
   },
   "eslintConfig": {
      "ignorePatterns": [
         "build",
         "dist",
         "node_modules"
      ],
      "root": true,
      "parser": "@typescript-eslint/parser",
      "plugins": [
         "@typescript-eslint"
      ],
      "extends": [
         "eslint:recommended",
         "plugin:@typescript-eslint/recommended"
      ],
      "rules": {
         "@typescript-eslint/no-non-null-assertion": "off"
      }
   },
   "scripts": {
      "step:01": "rimraf build dist **/.DS_Store",
      "step:02": "jshint . --exclude node_modules,hello-world/node_modules,build,dist",
      "step:03": "eslint --max-warnings 0 . --ext .ts",
      "step:04": "tsc",
      "step:05": "tsc --module UMD --outDir build/umd",
      "step:06": "cpy build/umd/puppeteer-browser-ready.js build --rename=puppeteer-browser-ready.umd.cjs --flat=true",
      "step:07": "add-dist-header build dist",
      "step:08": "w3c-html-validator",
      "pretest": "npm-run-all step:*",
      "test": "mocha spec/*.spec.js --timeout 5000"
   },
   "peerDependencies": {
      "puppeteer": "^5 || ^6 || ^7 || ^8 || ^9 || ^10 || ^11 || ^12 || ^13"
   },
   "dependencies": {
      "cheerio": "~1.0.0-rc.10",
      "express": "~4.17",
      "http-terminator": "~3.2"
   },
   "devDependencies": {
      "@types/cheerio": "~0.22",
      "@types/express": "~4.17",
      "@types/node": "~17.0",
      "@typescript-eslint/eslint-plugin": "~5.20",
      "@typescript-eslint/parser": "~5.20",
      "add-dist-header": "~0.1",
      "assert-deep-strict-equal": "~1.0",
      "cpy-cli": "~4.1",
      "eslint": "~8.13",
      "jshint": "~2.13",
      "mocha": "~9.2",
      "npm-run-all2": "~5.0",
      "open": "~8.4",
      "puppeteer": "~13.6",
      "rimraf": "~3.0",
      "typescript": "~4.6",
      "w3c-html-validator": "~1.0"
   }
}
